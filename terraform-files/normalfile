vi provider.tf

provider "aws" {
  region = "eu-north-1"
}

vi resource.tf

resource "aws_instance" "myserver" {
  tags = {
    Name = "fim-server"
    Env  = "Dev"
  }
  ami               = "ami-0854d4f8e4bd6b834"
  instance_type     = "t3.micro"
  key_name          = "newkey"
  availability_zone = "eu-north-1a"
  root_block_device {
    volume_size = 12
  }
}
terraform init
terraform plan
terraform apply --auto-approve
terraform destroy --auto-approve





