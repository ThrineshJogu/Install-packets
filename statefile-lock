vi provider.tf

provider "aws" {
  region = "eu-north-1"
}

vi resource.tf

resource "aws_instance" "myserver" {
  tags = {
    Name = "fim-server"
  }
  ami               = "ami-0854d4f8e4bd6b834"
  instance_type     = "t3.micro"
}

vi backend.tf

terraform {
backend "s3" {
bucket="bucket_name"
region="eu-nort-1"
key="prod/terraform.tfstate"
use_lockfile= true
}
}








